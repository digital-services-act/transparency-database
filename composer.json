{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-zip": "*",
        "consoletvs/charts": "6.*",
        "cweagans/composer-patches": "^1.7",
        "cybercog/laravel-clickhouse": "^0.1.0",
        "ec-doris/laravel-cas": "dev-main",
        "elasticsearch/elasticsearch": "^8.18",
        "erusev/parsedown": "^1.7",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.1",
        "laravel/tinker": "^2.8",
        "laravel/vapor-core": "^2.26",
        "league/flysystem-aws-s3-v3": "^3.19",
        "maatwebsite/excel": "^3.1",
        "nyholm/psr7": "^1.8",
        "predis/predis": "^2.1",
        "spatie/laravel-honeypot": "^4.3",
        "spatie/laravel-permission": "^6.0",
        "stevebauman/purify": "^6.0",
        "symfony/cache": "^7.0",
        "symfony/intl": "^7.0",
        "symfony/psr-http-message-bridge": "^7.0",
        "tightenco/ziggy": "^2.0",
        "timwassenburg/laravel-pivot-table-generator": "^1.0",
        "yoeriboven/laravel-log-db": "^1.2",
        "zing/laravel-scout-opensearch": "^3.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "brianium/paratest": "^7.0",
        "driftingly/rector-laravel": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "laravel-shift/blueprint": "^2.4",
        "laravel/boost": "^1.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0",
        "rector/rector": "^1.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "exclude-from-classmap": [
            "vendor/ec-doris/laravel-cas/src/Auth/CasGuard.php",
            "vendor/ec-doris/laravel-cas/src/Middleware/CasAuthenticator.php",
            "vendor/ec-doris/laravel-cas/src/Controllers/LoginController.php"
        ],
        "files": [
            "overrides/CasGuard.php",
            "overrides/CasAuthenticator.php",
            "overrides/LoginController.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "patches": {
            "apereo/phpcas": {
                "Ignore and bypass Session": "patches/apereo-phpcas.diff"
            },
            "subfission/cas": {
                "ServerBaseUrl": "patches/subfission-cas.diff"
            }
        }
    },
    "repositories": {
        "backpack": {
            "type": "composer",
            "url": "https://repo.backpackforlaravel.com/"
        },
        "ec-doris-laravel-cas": {
            "type": "vcs",
            "url": "https://github.com/ec-doris/laravel-cas"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "cweagans/composer-patches": true,
            "cyclonedx/cyclonedx-php-composer": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}